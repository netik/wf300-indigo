<?xml version="1.0"?>

<Devices>
	<Device type="dimmer" id="myRelayType">
		<Name>WF-300 LED Controller</Name>
		<ConfigUI>

			<Field id="address" type="textfield" defaultValue="192.168.2.2">
				<Label>Module Address:</Label>
			</Field>
		</ConfigUI>
		<States>

			<State id="programNumber">
				<ValueType>Integer</ValueType>
				<TriggerLabel>Program</TriggerLabel>
				<ControlPageLabel>Program</ControlPageLabel>
			</State>
			<State id="speed">
				<ValueType>Integer</ValueType>
				<TriggerLabel>Speed</TriggerLabel>
				<ControlPageLabel>Speed</ControlPageLabel>
			</State>

			<State id="pauseRun">
				<ValueType>Boolean</ValueType>
				<TriggerLabel>Pause</TriggerLabel>
				<ControlPageLabel>Pause</ControlPageLabel>
			</State>

		</States>
	</Device>

</Devices>
